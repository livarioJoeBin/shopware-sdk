  "description": "A PHP SDK for the Shopware 5 REST API.",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "replace": {
    "leadcommerce/shopware-sdk": "*",
    "net-io/shopware-sdk": "*"
  },
  "autoload": {
    "psr-0": {
      "livarioJoeBin\\Shopware\\SDK\\": "src/",
      "livarioJoeBin\\Tests\\Unit\\": "tests/",
      "livarioJoeBin\\Tests\\Functional\\": "tests/"
    }
  }
}
